{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "RateLimits": {
    "Window": 1,
    "Limit": 1
  },
  "CacheProfiles": {
    "Static": {
      "Duration": 3600
    },
    "Dynamic": {
      "Duration": 5
    }
  },
  "UpdateSchedule": {
    "Cities": "0 6 * * *",
    "StationsDelta": "1 6 2-31 * *",
    "StationsFull": "1 6 1 * *"
  },
  "MaxDesertedDays": 7
}
